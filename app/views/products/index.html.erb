<% content_for :main_heading_action_button do %>
  <%= render "shared/button", text: "New Product", link: new_product_path, style_class: 'Polaris-Button--primary' %>
<% end %>

<%= render partial: "shared/page_title", locals: { title: "Products" } %>

<% cache @products do %>
  <%= render partial: "shared/product", collection: @products, locals: { manage: true } %>
<% end %>



